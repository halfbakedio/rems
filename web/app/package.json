{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run graphql:codegen && node scripts/start.js",
    "build": "yarn run graphql:codegen && node scripts/build.js",
    "restart": "overmind restart app --socket ../../.overmind.sock",
    "test": "node scripts/test.js",
    "lint": "eslint --fix src/",
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:restart": "overmind restart storybook --socket ../../.overmind.sock"
  },
